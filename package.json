{
	"name": "material-symbols-font-composer",
	"version": "1.0.0",
	"description": "Compose your Material Symbols Font",
	"type": "module",
	"author": "Valentin Degenne <vdegenne@gmail.com>",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": ""
	},
	"devDependencies": {
		"@lit-labs/virtualizer": "^2.0.12",
		"@material/web": "^1.1.1",
		"@snar/lit": "^1.4.0",
		"@types/clean-css": "^4.2.11",
		"@types/node": "^20.11.5",
		"@vdegenne/about-dialog": "^0.4.2",
		"@vdegenne/material-color-helpers": "^1.6.8",
		"@vdegenne/rollup-plugin-minify-html": "^1.0.6",
		"clean-css": "^5.3.3",
		"custom-element-decorator": "^0.6.0",
		"lit": "^3.1.1",
		"lit-signals-helpers": "^2.0.1",
		"lit-with-styles": "^2.1.3",
		"material-3-prompt-dialog": "^0.11.1",
		"material-shell": "^1.2.3",
		"rollup-plugin-material-all": "^1.2.0",
		"rollup-plugin-minify-template-literals": "^1.1.4",
		"snar": "^2.2.0",
		"snar-save-to-local-storage": "^1.0.1",
		"toastit": "^0.1.0",
		"vite": "^4.5.1",
		"vite-plugin-md-icon": "^1.0.0",
		"wireit": "^0.14.3"
	},
	"scripts": {
		"dev": "wireit",
		"build": "wireit"
	},
	"wireit": {
		"dev": {
			"command": "vite --host",
			"files": [
				"vite.config.js"
			],
			"output": [
				"./node_modules/.vite"
			],
			"dependencies": [
				"download-symbols"
			]
		},
		"build": {
			"command": "vite build",
			"files": [
				"vite.config.js",
				"src",
				"index.html",
				"public"
			],
			"output": [
				"dist",
				"./node_modules/.vite"
			],
			"dependencies": [
				"download-symbols"
			]
		},
		"download-symbols": {
			"command": "node scripts/download-full-symbols-files.mjs",
			"files": [
				"scripts/*"
			],
			"output": [
				"public/material-symbols.woff2",
				"src/styles/stylesheets/symbols.css"
			]
		}
	}
}
